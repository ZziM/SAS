@using SAS.Web.Models
@using SAS.Model.Abstract

@model SAS.Web.Models.BusinessObjectCollectionViewModel<IAccessPoint, RequestAccessPointViewModel>

@Html.DevExpress().GridView(settings => {

    GridViewFunctions.ApplyDefault(settings, "gridViewGroupDetail" + ViewData["ID"]);
    settings.CallbackRouteValues = new { Controller = Model.Page.Controller, Action = Model.Page.Action, CustomerID = ViewData["CustomerID"] };

    settings.SettingsDetail.MasterGridName = "gvGroup";

    settings.KeyFieldName = "ID";
    settings.Columns.Add("Name");

}).Bind(Model.Model).GetHtml()